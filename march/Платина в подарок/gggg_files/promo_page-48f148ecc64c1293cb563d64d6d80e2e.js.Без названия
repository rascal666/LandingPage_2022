app.modules.showRegionsPopup=function(n){function o(){var n={regionHint:"hclm-region-hint",regionLink:"hclm-region-link js-region-link"};$(".js-show-regions-popup-wrapper[data-hint-url]").each(function(){var o=$(this),i=o.data();o.find(".js-show-regions-popup").after($("<div>").addClass(n.regionHint).text(i.hintText).append($("<a>").addClass(n.regionLink).text(i.hintRegionName).prop({href:i.hintUrl})))})}function i(){$doc.on("click",".js-show-regions-popup",function(){$doc.trigger("showRegionPopup",[$(this)])})}return n.load=function(){i(),o()},n}(app.modules.showRegionsPopup||{});